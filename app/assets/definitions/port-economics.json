      "storm": {
        "portClosureProbability": 0.3,
        "capacityReduction": 0.5,
        "priceSpike": 1.5,
        "affectedGoods": ["perishables", "electronics"],
        "duration": 7
      },
      "piracy": {
        "routeRiskIncrease": 0.2,
        "insuranceCostMultiplier": 1.5,
        "priceIncrease": 1.1,
        "affectedGoods": ["luxury", "electronics"],
        "duration": 30
      },
      "strike": {
        "portEfficiencyReduction": 0.7,
        "capacityReduction": 0.8,
        "priceIncrease": 1.2,
        "affectedGoods": ["all"],
        "duration": 14
      },
      "pandemic": {
        "globalDemandShift": 0.8,
        "supplyChainDisruption": 0.6,
        "priceVolatilityIncrease": 2.0,
        "affectedGoods": ["pharmaceuticals", "medical_equipment"],
        "duration": 180
      }
    },
    "economicEvents": {
      "boom": {
        "demandMultiplier": 1.3,
        "priceStability": 0.8,
        "tradeVolumeIncrease": 1.4,
        "creditAvailability": 1.5
      },
      "recession": {
        "demandMultiplier": 0.7,
        "priceVolatility": 1.5,
        "tradeVolumeDecrease": 0.8,
        "creditAvailability": 0.6
      },
      "trade_war": {
        "tariffIncrease": 1.25,
        "tradeVolumeDecrease": 0.85,
        "priceIncrease": 1.15,
        "affectedRoutes": ["transpacific", "asia_europe"]
      },
      "oil_shock": {
        "fuelCostMultiplier": 2.0,
        "transportCostIncrease": 1.5,
        "demandDecrease": 0.9,
        "affectedGoods": ["crude_petroleum", "refined_petroleum"]
      }
    },
    "portCompetition": {
      "factors": {
        "efficiency": 0.3,
        "location": 0.25,
        "infrastructure": 0.2,
        "cost": 0.15,
        "connections": 0.1
      },
      "competitionRadius": 500,
      "marketShareCalculation": "relative_attractiveness"
    },
    "priceFormula": {
      "base": "basePrice * (1 + supplyDemandModifier)",
      "supplyDemandModifier": "(demand - supply) / (demand + supply) * priceElasticity",
      "transportCost": "distance * transportCostFactor * fuelPrice",
      "finalPrice": "base + transportCost + portFees + insurance"
    }
  },
